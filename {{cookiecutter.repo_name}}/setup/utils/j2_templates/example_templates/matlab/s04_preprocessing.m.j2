function s04_preprocessing()
    paths = get_paths();
    ensureDir(paths.interim);

    data = loadData(fullfile(paths.raw, 'iris_raw.mat'));
    data = data(ismember(data.Species, {'setosa', 'versicolor'}), :);

    saveData(fullfile(paths.interim, 'iris_clean.mat'), data);
    disp('Preprocessing complete.');
end
